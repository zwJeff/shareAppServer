<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jeff.model.AdsStatusModel" table="ads_status" schema="shareapp_db">
        <id name="key">
            <column name="key" sql-type="varchar(255)"/>
        </id>
        <property name="status">
            <column name="status" sql-type="int(11)"/>
        </property>
        <property name="comment">
            <column name="comment" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="unitId">
            <column name="unit_id" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>